<h1>Listing Rooms</h1>

<%= link_to 'New room', new_room_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @rooms.each do |room| %>
    <tr>
      <td><%= room.title %></td>
      <td><%= room.text %></td>
      <td><%= link_to 'Show', room_path(room) %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Back', rooms_path %>